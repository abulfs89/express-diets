<%- include('../partials/header') %>
<body>
  <h2>Diet List</h2>
  <a href="/diets/new">Add Diet</a>
  <ul>
    <%diets.forEach(diet=>{%>
      <li><a href="diets/<%=diet.id%>"><%=diet.name%></a></li>
     <form action="/diets/<%=diet.id%>?_method=DELETE" method="POST"> 
         <input type="submit" value="X"/> 
      </form>
    <%})%>
  </ul>
</body>
</html>

